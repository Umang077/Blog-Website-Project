<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input type="text" name="postTitle" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="">
      <label>Post</label>
      <textarea class="form-control" name="postBody" cols="40" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
